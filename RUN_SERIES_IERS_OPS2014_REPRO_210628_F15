echo " #  $SCR_NAME $* "
echo " # "
echo " #  Version $SCR_VERS "
echo " # "
echo " #  Run Date $DateTime "
echo " # "


CASE=${1:-WEEKLY}
ETA_IN=${2:-YES}
GROUP=${3:-REANAL14}
JOB_FILE=${4:-}
RUN_NUMBER=$5
RUN_YEAR=$6
#MDLS=${7:-1810}    #      This is the TNM05 set !!!
TSTN=${7:-TNM01}

