echo "NEW P-SERIES2010 2014 RESULTS FOR ARC: " $ARC > thdr
cat thdr /${DISK}/PPV/products/$CASE/OPS/${SERVICE}_${SOL_EN}_/${ARC}.JCET > 2mail

